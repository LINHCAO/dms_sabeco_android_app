<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" android:constantSize="true">
 <!-- enable checked -->
    <item android:drawable="@drawable/icon_checkbox2_on" android:state_checked="true" android:state_enabled="true"/>
 <!-- enable unchecked -->
    <item android:drawable="@drawable/icon_checkbox2_off" android:state_checked="false" android:state_enabled="true"/>
 <!-- disable checked -->
    <item android:drawable="@drawable/icon_checkbox2_on_disable" android:state_checked="true" android:state_enabled="false"/>
 <!-- disable unchecked-->
 	<item android:drawable="@drawable/icon_checkbox2_off_disable" android:state_checked="false" android:state_enabled="false"/>

</selector>