<?xml version="1.0" encoding="utf-8"?>
<TableRow xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/row"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@color/TABLE_BG"
    android:minWidth="776dip" >

    <TextView
        android:id="@+id/tvMaCT"
        style="@style/TextViewVinamilkTable"
        android:layout_width="130dip"
        android:layout_height="fill_parent"
        android:gravity="center_vertical|left"
        android:minHeight="35dip"
        android:padding="5dip"
        android:text="MÃ£ CT"
        android:textColor="@color/COLOR_LINK"
        />

    <TextView
        android:id="@+id/tvTenCT"
        style="@style/TextViewVinamilkTable"
        android:layout_width="250dip"
        android:layout_height="wrap_content"
        android:gravity="center_vertical|left"
        android:minHeight="35dip"
        android:padding="5dip"
        android:paddingLeft="10dip"
        android:text="" />

    <TextView
        android:id="@+id/tvDate"
        style="@style/TextViewVinamilkTable"
        android:layout_width="190dip"
        android:layout_height="fill_parent"
        android:gravity="center_vertical|left"
        android:minHeight="35dip"
        android:padding="5dip"
        android:paddingLeft="10dip"
        android:text="" />

    <LinearLayout android:duplicateParentState="true"
        android:id="@+id/layoutLevel"
        android:layout_width="wrap_content"
        android:layout_height="fill_parent"
        android:minHeight="35dip"
        android:orientation="horizontal" >
    </LinearLayout>

    <TextView
        android:id="@+id/tvTotal"
        style="@style/TextViewVinamilkTable"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:gravity="center"
        android:minHeight="35dip"
        android:text="" />

</TableRow>