<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content" >

    <HorizontalScrollView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@color/BG_FRAGMENT"
        android:padding="10dip" >

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@color/BG_FRAGMENT"
            android:orientation="vertical"
            android:padding="10dip" >

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@color/TABLE_BG"
                android:padding="2dip" >

                <TextView
                    android:id="@+id/tvMaNVBH"
                    android:layout_width="110dip"
                    android:layout_height="fill_parent"
                    android:background="@color/TABLE_HEADER_BG"
                    android:gravity="center"
                    android:minHeight="35dip"
                    android:text="@string/TEXT_COLUM_NVBH_CODE"
                    android:textColor="@color/BLACK"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/tvTenNVBH"
                    android:layout_width="160dip"
                    android:layout_height="fill_parent"
                    android:layout_marginLeft="2dip"
                    android:background="@color/TABLE_HEADER_BG"
                    android:gravity="center"
                    android:minHeight="35dip"
                    android:padding="5dip"
                    android:text="@string/TEXT_HEADER_TABLE_NVBH"
                    android:textColor="@color/BLACK"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/tvDate"
                    android:layout_width="120dip"
                    android:layout_height="fill_parent"
                    android:layout_marginLeft="2dip"
                    android:background="@color/TABLE_HEADER_BG"
                    android:gravity="center"
                    android:minHeight="35dip"
                    android:padding="5dip"
                    android:text="@string/TEXT_TRAINING_DATE"
                    android:textColor="@color/BLACK"
                    android:textStyle="bold" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="2dip"
                    android:orientation="vertical" >

                    <TextView
                        android:id="@+id/tvDVT"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/TABLE_HEADER_BG"
                        android:gravity="center"
                        android:minHeight="35dip"
                        android:padding="5dip"
                        android:text="@string/TEXT_DAILY_AMOUNT"
                        android:textColor="@color/BLACK"
                        android:textStyle="bold" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal" >

                        <TextView
                            android:id="@+id/tvPlanAmount"
                            android:layout_width="105dip"
                            android:layout_height="fill_parent"
                            android:layout_marginTop="2dip"
                            android:background="@color/TABLE_HEADER_BG"
                            android:gravity="center"
                            android:minHeight="35dip"
                            android:text="@string/TEXT_HEADER_TABLE_PLAN"
                            android:textColor="@color/BLACK"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/tvAmount"
                            android:layout_width="105dip"
                            android:layout_height="fill_parent"
                            android:layout_marginLeft="2dip"
                            android:layout_marginTop="2dip"
                            android:background="@color/TABLE_HEADER_BG"
                            android:gravity="center"
                            android:minHeight="35dip"
                            android:padding="5dip"
                            android:text="@string/TEXT_HEADER_TABLE_DONE"
                            android:textColor="@color/BLACK"
                            android:textStyle="bold" />
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="2dip"
                    android:orientation="vertical" >

                    <TextView
                        android:id="@+id/tvNumberShop"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/TABLE_HEADER_BG"
                        android:gravity="center"
                        android:minHeight="35dip"
                        android:padding="5dip"
                        android:text="@string/TEXT_STORE_NUM"
                        android:textColor="@color/BLACK"
                        android:textStyle="bold" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal" >

                        <TextView
                            android:id="@+id/tvKH"
                            android:layout_width="50dip"
                            android:layout_height="fill_parent"
                            android:layout_marginTop="2dip"
                            android:background="@color/TABLE_HEADER_BG"
                            android:gravity="center"
                            android:minHeight="35dip"
                            android:text="@string/TEXT_CUSTOMER_"
                            android:textColor="@color/BLACK"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/tvPSDS"
                            android:layout_width="50dip"
                            android:layout_height="fill_parent"
                            android:layout_marginLeft="2dip"
                            android:layout_marginTop="2dip"
                            android:background="@color/TABLE_HEADER_BG"
                            android:gravity="center"
                            android:minHeight="35dip"
                            android:padding="5dip"
                            android:text="@string/TEXT_PSDS"
                            android:textColor="@color/BLACK"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/tvMoi"
                            android:layout_width="50dip"
                            android:layout_height="fill_parent"
                            android:layout_marginLeft="2dip"
                            android:layout_marginTop="2dip"
                            android:background="@color/TABLE_HEADER_BG"
                            android:gravity="center"
                            android:minHeight="35dip"
                            android:padding="5dip"
                            android:text="@string/TEXT_NEW"
                            android:textColor="@color/BLACK"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/tvON"
                            android:layout_width="50dip"
                            android:layout_height="fill_parent"
                            android:layout_marginLeft="2dip"
                            android:layout_marginTop="2dip"
                            android:background="@color/TABLE_HEADER_BG"
                            android:gravity="center"
                            android:minHeight="35dip"
                            android:padding="5dip"
                            android:text="@string/TEXT_ON"
                            android:textColor="@color/BLACK"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/tvNT"
                            android:layout_width="50dip"
                            android:layout_height="fill_parent"
                            android:layout_marginLeft="2dip"
                            android:layout_marginTop="2dip"
                            android:background="@color/TABLE_HEADER_BG"
                            android:gravity="center"
                            android:minHeight="35dip"
                            android:padding="5dip"
                            android:text="@string/TEXT_NT"
                            android:textColor="@color/BLACK"
                            android:textStyle="bold" />
                    </LinearLayout>
                </LinearLayout>

                <TextView
                    android:layout_width="65dip"
                    android:layout_height="fill_parent"
                    android:layout_marginLeft="2dip"
                    android:background="@color/TABLE_HEADER_BG"
                    android:gravity="center"
                    android:minHeight="35dip"
                    android:padding="5dip"
                    android:text="@string/TEXT_POINT"
                    android:textColor="@color/BLACK"
                    android:textStyle="bold" >
                </TextView>
            </LinearLayout>

            <com.viettel.dms.view.control.VinamilkTableView
                android:id="@+id/tbCusList"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="-3dip" >
            </com.viettel.dms.view.control.VinamilkTableView>
        </LinearLayout>
    </HorizontalScrollView>

</ScrollView>