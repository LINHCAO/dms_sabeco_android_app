<?xml version="1.0" encoding="utf-8"?>
<TableRow xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/row"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@color/TABLE_BG"
    android:minHeight="35dip" >
    
    <TextView
        android:id="@+id/tvTong"
        style="@style/TextViewVinamilkTable"
        android:layout_width="550dip"
        android:layout_height="fill_parent"        
        android:text="@string/TEXT_HEADER_TABLE_RESULT_TOTAL"
        android:gravity="center|center_vertical" />

    <TextView
        android:id="@+id/tvMucTieu"
        style="@style/TextViewVinamilkTable"
        android:layout_width="90dip"
        android:layout_height="fill_parent"
        android:gravity="center|center_vertical" />

    <TextView
        android:id="@+id/tvThucHien"
        style="@style/TextViewVinamilkTable"
        android:layout_width="88dip"
        android:layout_height="fill_parent"
        android:gravity="center|center_vertical"
        android:paddingRight="10dip" />

    <TextView
        android:id="@+id/tvDiem"
        style="@style/TextViewVinamilkTable"
        android:layout_width="70dip"
        android:layout_height="fill_parent"
        android:gravity="center|center_vertical"
        android:paddingRight="10dip" />

    <TextView
        android:id="@+id/tvMoi"
        style="@style/TextViewVinamilkTable"
        android:layout_width="50dip"
        android:layout_height="fill_parent"        
        android:gravity="center|center_vertical"
        android:paddingRight="10dip" />

    <TextView
        android:id="@+id/tvOn"
        style="@style/TextViewVinamilkTable"
        android:layout_width="50dip"
        android:layout_height="fill_parent"
        android:gravity="center|center_vertical" />
    <TextView
        android:id="@+id/tvNT"
        style="@style/TextViewVinamilkTable"
        android:layout_width="46dip"
        android:layout_height="fill_parent"
        android:gravity="center|center_vertical" />
<TextView
        android:id="@+id/tvNoResultInfo"
        style="@style/TextViewVinamilkTable"
        android:layout_width="956dip"
        android:layout_height="fill_parent"
        android:gravity="center"
        android:padding="@dimen/padding"
        android:visibility="gone"
         />
</TableRow>