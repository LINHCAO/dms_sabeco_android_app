<?xml version="1.0" encoding="utf-8"?>
<TableRow xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/row"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@color/TABLE_BG"
    android:minHeight="35dip" >

    <TextView
        android:id="@+id/tvSTT"
        style="@style/TextViewVinamilkTable"
        android:layout_width="50dip"
        android:layout_height="fill_parent"
        android:gravity="center"
        android:padding="5dip"/>
    <TextView
        android:id="@+id/tvMaNvbh"
        style="@style/TextViewVinamilkTable"
        android:layout_width="70dip"
        android:layout_height="fill_parent"
        android:gravity="center"
        android:padding="5dip"
        android:text="@string/TEXT_HEADER_TABLE_NVBH_CODE"
        android:textColor="@color/COLOR_USER_NAME" />

    <TextView
        android:id="@+id/tvNvbh"
        style="@style/TextViewVinamilkTable"
        android:layout_width="212dip"
        android:layout_height="fill_parent"
        android:gravity="center_vertical|left"
        android:paddingLeft="10dip"
        android:text="@string/TEXT_NVBH" />

    <TextView
        android:id="@+id/tvDiaChi"
        style="@style/TextViewVinamilkTable"
        android:layout_width="212dip"
        android:layout_height="fill_parent"
        android:paddingLeft="10dip"
        android:gravity="left|center_vertical" />

    <TextView
        android:id="@+id/tvMucTieu"
        style="@style/TextViewVinamilkTable"
        android:layout_width="90dip"
        android:layout_height="fill_parent"
        android:gravity="center|center_vertical" />

    <TextView
        android:id="@+id/tvThucHien"
        style="@style/TextViewVinamilkTable"
        android:layout_width="88dip"
        android:layout_height="fill_parent"
        android:gravity="center|center_vertical"
        android:paddingRight="10dip" />

    <TextView
        android:id="@+id/tvDiem"
        style="@style/TextViewVinamilkTable"
        android:layout_width="70dip"
        android:layout_height="fill_parent"
        android:gravity="center|center_vertical"
        android:paddingRight="10dip" />

    <TextView
        android:id="@+id/tvMoi"
        style="@style/TextViewVinamilkTable"
        android:layout_width="50dip"
        android:layout_height="fill_parent"        
        android:gravity="center|center_vertical"
        android:paddingRight="10dip" />

    <TextView
        android:id="@+id/tvOn"
        style="@style/TextViewVinamilkTable"
        android:layout_width="50dip"
        android:layout_height="fill_parent"
        android:gravity="center|center_vertical" />
    <TextView
        android:id="@+id/tvNT"
        style="@style/TextViewVinamilkTable"
        android:layout_width="46dip"
        android:layout_height="fill_parent"
        android:gravity="center|center_vertical" />

</TableRow>