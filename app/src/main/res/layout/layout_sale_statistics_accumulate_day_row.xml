<?xml version="1.0" encoding="utf-8"?>
<TableRow xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/row"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@color/TABLE_BG"
    android:minHeight="35dp" >

    <!-- android:background="@color/TABLE_BG" > -->

    <TextView
        android:id="@+id/tvStt"
        style="@style/TextViewVinamilkTable"
        android:layout_width="45dip"
        android:layout_height="fill_parent"
        android:gravity="center"
        android:padding="5dip"
        android:text="Mã NVBH" />

    <TextView
        android:id="@+id/tvMaHang"
        style="@style/TextViewVinamilkTable"
        android:layout_width="90dip"
        android:layout_height="fill_parent"
        android:gravity="center"
        android:padding="5dip"
        android:text="Mã NVBH" />

    <TextView
        android:id="@+id/tvTenMatHang"
        style="@style/TextViewVinamilkTable"
        android:layout_width="210dip"
        android:layout_height="fill_parent"
        android:gravity="center_vertical|left"
        android:paddingLeft="10dip"
        android:text="NVBH" />

    <TextView
        android:id="@+id/tvNH"
        style="@style/TextViewVinamilkTable"
        android:layout_width="55dip"
        android:layout_height="fill_parent"
        android:gravity="center"
        android:paddingLeft="10dip" />

    <TextView
        android:id="@+id/tvKeHoach"
        style="@style/TextViewVinamilkTable"
        android:layout_width="140dip"
        android:layout_height="fill_parent"
        android:gravity="center"
        android:paddingLeft="10dip" />

    <TextView
        android:id="@+id/tvThucHien"
        style="@style/TextViewVinamilkTable"
        android:layout_width="140dip"
        android:layout_height="fill_parent"
        android:gravity="center" />

    <TextView
        android:id="@+id/tvConLai"
        style="@style/TextViewVinamilkTable"
        android:layout_width="140dip"
        android:layout_height="fill_parent"
        android:gravity="center" />

    <TextView
        android:id="@+id/tvTienDo"
        style="@style/TextViewVinamilkTable"
        android:layout_width="127dip"
        android:layout_height="fill_parent"
        android:gravity="right|center_vertical"
        android:paddingRight="10dip" />

</TableRow>