<?xml version="1.0" encoding="utf-8"?>
<TableRow xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/row"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@color/TABLE_BG" >

    <TextView
        android:id="@+id/tvMaNVBH"
        style="@style/TextViewVinamilkTable"
        android:layout_width="100dip"
        android:layout_height="match_parent"
        android:gravity="center"
        android:padding="5dip"
        android:text=""
        android:minHeight="40dip"
        android:textColor="@color/COLOR_USER_NAME" />

    <TextView
        android:id="@+id/tvNVBH"
        style="@style/TextViewVinamilkTable"
        android:layout_width="150dip"
        android:layout_height="match_parent"
        android:gravity="center_vertical|left"
        android:paddingLeft="10dip"
        android:minHeight="40dip"
        android:text="Tổng cộng" />
    
    <TextView
        android:id="@+id/tvTotal"
        style="@style/TextViewVinamilkTable"
        android:layout_width="262dip"
        android:layout_height="match_parent"
        android:gravity="center_vertical|right"
        android:paddingLeft="10dip"
        android:paddingRight="10dip"
        android:textStyle="bold"
        android:visibility="gone"
        android:minHeight="40dip"
        android:text="@string/TOTAL" />
    <TextView
        android:id="@+id/tvNotifyNull"
        style="@style/TextViewVinamilkTable"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:minHeight="@dimen/min_height_of_row"
        android:padding="@dimen/padding"
        android:textColor="@color/BLACK"
        android:visibility="gone" />
    
    
    <LinearLayout android:duplicateParentState="true"
        android:id="@+id/layMHTT"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        >
    </LinearLayout>

